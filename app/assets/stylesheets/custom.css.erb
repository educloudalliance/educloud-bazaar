body {
    padding-top: 70px;
}

.back-material-list {
    margin-bottom: 10px;
}

.form-signin {
    max-width: 230px;
    padding: 15px;
    margin: 10% auto;
    text-align: center;
}

.my-cart, .my-cart:hover {
    text-decoration: none;
    color: #337ab7;
}

.cart-messages {
    position: fixed;
    top: 100px;
    right: 30px;
    display: none;
}

.margin-bottom-10 {
    margin-bottom: 10px;
}

.text-red {
    color: red;
}

.margin-top-10 {
    margin-top: 10px;
}

.wrapper {
    background: url(<%= asset_path('background.png') %>) no-repeat top left fixed;
    background-size: contain;
    min-width: 100%;
    min-height: 100%;
}

.vmiddle {
    vertical-align: middle;
}